<h1>Jobs#index</h1>
<p>Find me in app/views/jobs/index.html.erb</p>

<div class="">
  <% @jobs.each do |job| %>
  <ul>
    <h3><%= job.summary %></h3>
    <li><%= job.detail %></li>
    <li><%= job.address %></li>
    <li><%= job.requested_date %></li>
    <li><%= job.accepted_date %></li>
    <li><%= job.finished %></li>
    <li><%= job.booker_id %></li>
    <li><%= job.technician_id %></li>
  </ul>
  <% end %>
</div>
